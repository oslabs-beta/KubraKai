{
 "name": "KubraKai",
 "version": "1.0.0",
 "description": "A GUI for Kubernetes monitoring",
 "main": "index.js",
 "scripts": {
  "react": "cd node_modules/react-scripts && node bin/react-scripts.js start",
  "start": "cross-env NODE_ENV=production nodemon server/server.js",
  "build": "webpack --config webpack.config.js",
  "dev": "concurrently \"webpack --config webpack.config.js\" \"webpack serve --hot --open\" \"nodemon server/server.js\"",
  "test": "jest --verbose"
 },
 "nodemonConfig": {
  "ignore": [
